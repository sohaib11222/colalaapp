{
  "expo": {
    "name": "Colala Mall",
    "slug": "colala-mall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "splash": {
      "image": "./assets/appicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.colala.express",
      "icon": "./assets/appicon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.colala.express"
    },
    "web": {
      "favicon": "./assets/appicon.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9eed35ab-65f7-4dd3-a5e3-2c5007fdb1a7"
      }
    }
  }
}
